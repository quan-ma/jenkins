node('master'){
    stage('import'){
        try{
		git 'https://github.com/quan-ma/jenkins.git'
		} catch (e) {
		//slackSend message:{env.BUILD_NUMBER} color: 'danger'

		throw error
		}
    }   
    
    stage('build'){
        try{
			dir('Jenkinz'){
				bat 'dotnet restore'
				bat 'msbuild /t:clean,build Jenkinz.csproj'
			}

		} catch (e){
		//slackSend message: color: 'danger'
		}
    }
    
    stage('analyze'){
        try{
			dir('Jenkinz'){
				
			}
		} catch (e){
		//slackSend message: color: 'danger'
		}
    }

	stage('test'){
	
	}
    
    stage('package'){
        
		try{
			dir('Jenkinz'){
				bat 'msbuild /t:package Jenkinz.csproj'
			}
		} catch (e){
			//slackSend message: color: 'danger'
		}

    }
    
    stage('deploy'){
        
    }
}